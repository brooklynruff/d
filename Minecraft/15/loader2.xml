<?xml version="1.0" encoding="UTF-8"?>
<Module>
<ModulePrefs title="BitLife"/>
<Content type="html"><![CDATA[

<html>

<head>
	<style type="text/css">

button {
  align-items: center;
  background-image: linear-gradient(144deg,#AF40FF, #5B42F3 50%,#00DDEB);
  border: 0;
  border-radius: 8px;
  box-shadow: rgba(151, 65, 252, 0.2) 0 15px 30px -5px;
  box-sizing: border-box;
  color: #FFFFFF;
  display: flex;
  font-family: Phantomsans, sans-serif;
  font-size: 20px;
  justify-content: center;
  line-height: 1em;
  width: 100%;
  padding: 3px;
  text-decoration: none;
  user-select: none;
  -webkit-user-select: none;
  touch-action: manipulation;
  white-space: nowrap;
  cursor: pointer;
  margin: 0;
  position: absolute;
  top: 50%;
  left: 50%;
  ms-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

button:active,
button-64:hover {
  outline: 0;
}

button span {
  background-color: rgb(5, 6, 45);
  padding: 16px 24px;
  border-radius: 6px;
  width: 100%;
  height: 100%;
  transition: 300ms;
}

button:hover span {
  background: none;
}

@media (min-width: 768px) {
  .button-64 {
    font-size: 24px;
    min-width: 196px;
  }
}

	</style>
</head>



<script>

async function full() {
  let win = window;

  win.document.documentElement.innerHTML = ""
  win.document.body.innerHTML = "<h1>Downloading, this may take a second... (It may take longer if this is your first time)</h1>";

  let url = "https://cdn.jsdelivr.net/gh/brooklynruff/d@main/Minecraft/15/eagle5.html";
  let lcStorage = localStorage.getItem(url);
  let body;
  
  if (lcStorage) {
    body = lcStorage
  } else {
    body = await (await fetch(url)).text()

    localStorage.setItem(url, body)
  }
  win.document.body.innerHTML = ""
  win.document.write(body);
}

</script>

<button onclick="full();">Launch</button>


</html>
]]></Content>
</Module>